      "!src/**/static/**",
      "!src/**/Mock*.ts",
      "!**/Fixture*.ts",
      "!src/config/**",
      "!src/**/constants/**",
      "!src/setupTests.js",
      "!src/serviceWorker.ts",
      "!src/utils/data/**"
    ]
  },
