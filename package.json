  "scripts": {
    "generate-build-version": "node generate-build-version",
    "start": "react-app-rewired start",
    "prebuild": "npm version patch  && npm run generate-build-version",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "cypress:open": "cypress open",
    "test:e2e": "cypress run",
    "test:e2e:ci": "concurrently --kill-others --success=first 'yarn start --progress=false --no-info' 'sleep 120 && yarn test:e2e'",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public",
    "lint": "eslint --ignore-path .eslintignore .",
    "lint:report": "eslint --ignore-path .eslintignore -f json -o report.json .",
    "coverage": "react-scripts test --coverage --watchAll=false",
    "dsten:account": "yarn dtsgen --out ./src/services/models/AccountApiModel.d.ts ./swagger/account-service.json --namespace AccountApiDefinitions",
    "dsten:customer": "yarn dtsgen --out ./src/services/models/CustomerApiModel.d.ts ./swagger/customer-service.json --namespace CustomerApiDefinitions",
    "dsten:profile": "yarn dtsgen --out ./src/services/models/ProfileApiModel.d.ts ./swagger/profile-service.json --namespace ProfileApiDefinitions",
    "dsten:data": "yarn dtsgen --out ./src/services/models/DataApiModel.d.ts ./swagger/data-service.json --namespace DataApiDefinitions",
    "dsten:games": "yarn dtsgen --out ./src/services/models/GamesApiModel.d.ts ./swagger/games-service.json --namespace GamesApiDefinitions",
    "dsten:notifications": "yarn dtsgen --out ./src/services/models/NotificationsApiModel.d.ts ./swagger/notifications-service.json --namespace NotificationsApiDefinitions",
    "dsten:messages": "yarn dtsgen --out ./src/services/models/MessagesApiModel.d.ts ./swagger/messages-service.json --namespace MessagesApiDefinitions",
    "translate": "node ./scripts/translation/index.js ./src/common/locales/,./src/features/authentication/locales/,./src/features/settings/locales/,./src/features/users/locales/,./src/features/events/locales/ en fr,es",
    "postinstall": "patch-package"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
